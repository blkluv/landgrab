<%- headers = ['Post View ID', 'Created At', 'User ID', 'Post ID'] -%>
<%= CSV.generate_line headers -%>
<%- @post_views.find_each do |post_view| -%>
<%= CSV.generate_line([
    post_view.hashid,
    post_view.created_at,
    User.encode_id(post_view.user_id),
    Post.encode_id(post_view.post_id)
  ]).html_safe -%>
<%- end -%>
